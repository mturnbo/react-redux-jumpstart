    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@babel/runtime": "^7.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn lint"
    }
  },
  "scripts": {
    "dev": "webpack-dev-server --mode development --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.prod.js",
    "lint": "eslint --fix --cache --ignore-path .gitignore --ext .jsx,.js .",
    "test": "jest --config=jest.config.json",
    "test:update": "jest --config=jest.config.json --update-snapshot",
    "test:watch": "jest --config=jest.config.json --coverage --watchAll",
    "dockerbuild": "webpack --mode production --config webpack.prod.js && docker build -t react-redux-jumpstart:latest .",
    "storybook": "start-storybook -s ./assets -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "src/index.js"
