{
  "name": "react-redux-jumpstart",
  "description": "React Redux App Jumpstart.",
  "version": "1.21.0",
  "author": "Marcus Turnbo <marcus@emarcus.com>",
  "keywords": [
    "react",
    "redux",
    "babel",
    "webpack",
    "jest",
    "enzyme",
    "docker",
    "nginx",
    "boilerplate",
    "jumpstart"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mturnbo/react-redux-jumpstart.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.9",
    "@storybook/addon-links": "^4.1.9",
    "@storybook/addons": "^4.1.9",
    "@storybook/react": "^4.1.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-enzyme": "^7.0.2",
    "jshint": "^2.10.2",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "prettier": "1.16.4",
    "react-test-renderer": "^16.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "classnames": "^2.2.6",
    "core-js": "^3.1.3",
    "lodash": "^4.17.11",
    "lorem-ipsum": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "uikit": "^3.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn lint"
    }
  },
  "jshintConfig": {
    "browser": true,
    "browserify": true,
    "devel": true,
    "curly": true,
    "eqeqeq": true,
    "eqnull": true,
    "undef": true,
    "unused": true,
    "plusplus": true,
    "quotmark": "single",
    "esversion": 6
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "eslint --fix --cache --ignore-path .gitignore --ext .jsx,.js .",
    "test": "jest --config=jest.config.js --coverage",
    "test:update": "jest --config=jest.config.js --update-snapshot",
    "test:watch": "jest --config=jest.config.js --coverage --watchAll",
    "dockerbuild": "webpack --mode production --config webpack.prod.js && docker build -t react-redux-jumpstart:latest .",
    "storybook": "start-storybook -s ./assets -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "src/index.js"
}
